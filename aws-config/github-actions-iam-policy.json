{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowIAMRoleForLambda",
      "Effect": "Allow",
      "Action": [
        "iam:CreateRole",
        "iam:DeleteRole",
        "iam:GetRole",
        "iam:PassRole",
        "iam:AttachRolePolicy",
        "iam:DetachRolePolicy",
        "iam:PutRolePolicy",
        "iam:DeleteRolePolicy",
        "iam:GetRolePolicy",
        "iam:ListRolePolicies",
        "iam:ListAttachedRolePolicies",
        "iam:TagRole",
        "iam:UntagRole"
      ],
      "Resource": [
        "arn:aws:iam::*:role/christopher-corbin-portfolio-backend-*",
        "arn:aws:iam::*:role/christopher-corbin-portfoli-*",
        "arn:aws:iam::*:role/*ContactFormFunctionRole*"
      ]
    }
  ]
}